{
    "name": "dicr/yii2-validate",
    "type": "yii2-extension",
    "license": "GPL-3.0-or-later",
    "description": "Компоненты парсинга и валидации данных для Yii2",
    "keywords": ["dicr", "yii2", "validate"],
    "homepage": "https://gitlab.com/dicr/yii2-validate",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/dicr/yii2-validate.git"
    },
    "autoload": {
        "psr-4": {
            "dicr\\validate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dicr\\tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "~7.4 || ~8.0.1",
        "yiisoft/yii2": "~2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.5"
    },
    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"}
    ]
}
